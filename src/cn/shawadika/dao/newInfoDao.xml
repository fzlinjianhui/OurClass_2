<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.shawadika.dao.newInfoDao">
	<!--  -->
	<insert id="insertNewInfo" parameterType="cn.shawadika.entity.newInfo">
		insert into newinfo values(
			#(type),
			#(title),
			#(content),
			#(time),
			#(ineffect),
			#(userId),
			#(res1),
			#(res2)
		);
	</insert>
</mapper>